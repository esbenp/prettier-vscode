{
  "env": {
      "es6": true,
      "node": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
      "prettier": [
          true,
          {
              "singleQuote": true,
              "trailingComma": "es5",
              "semi": false
          }
      ],
      "semicolon": [true, "never"],
      "quotemark": [true, "single"]
  }
}
